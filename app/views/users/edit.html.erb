<h1>Editing Users</h1>
<%= form_for(@user) do |f| %>
    <div class="field">
      <%= f.label :username %><br>
      <%= f.text_field :username %>
    </div>
    <div class="field">
      <%= f.label :password %><br>
      <%= f.text_field :password %>
    </div>
    <div class = "button">
    	<%= f.submit %>	
    </div>
<% end %>
redirect_to :back